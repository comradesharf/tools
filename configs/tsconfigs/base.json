{
    "compilerOptions": {
        "target": "esnext",
        "lib": ["esnext"],
        "allowJs": true,
        "skipLibCheck": true,
        "strict": true,
        "noEmit": true,
        "esModuleInterop": true,
        "module": "esnext",
        "moduleResolution": "Bundler",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "incremental": true,
        "allowImportingTsExtensions": true
    }
}
